   <string notr="true">/* General */
* {
	margin: 0;
	padding: 0;
	font-size: 16px;
	outline: none;
	font-family: &quot;Arial&quot;;
}

QFrame {
	border: none;
}

#main_body {
	background-color: rgba(32,36,42,255);
	border-radius: 10%;
}

#book_info {
	background-color: rgb(76,79,86);
	border-radius: 10%;
}

#error_label {
	margin-top: 15px;
}

/* Tabs style */
QTabWidget QLabel {
	font-size: 16px;
}

QTabWidget::pane {
	background: transparent;
	border: none;
	overflow: hidden;
}

QTabWidget::tab-bar {
    alignment: center;
}

QTabBar::tab {
	background-color: rgb(100,100,116);
    color: white;
    border-radius: 5px;
	width: 200px;
	height: 35px;
	padding: 0px 10px;
	margin: 0 5px;
	font-weight: 500;
}

QTabBar::tab:selected {
    background: rgba(254,211,44,255);
	color: black;
}

#tab_background, #tab_background_2, #tab_background_3 {
	background-color: rgb(76,79,86);
	border-radius: 10%;
}

/* Qlabel style */
QLabel {
	color: white;
}

#book_title {
	font-size: 20px;
	font-weight: 500;
}

/* Buttons style */
QPushButton {
	background: rgba(254,211,44,255);
	border-radius: 5px;
	padding: 0 10px 0 10px;
	height: 40px;
	color: black;
}

QPushButton:pressed {
	background: rgba(204,161, 44,255);
	color: white;
}

/* Text input style */
QLineEdit {
	border-radius: 10%;
	background-color: rgba(32,36,42,255);
	color: white;
	padding-left: 10px;
	height: 40px;
}

QLineEdit:focus {
	border: 2px solid rgba(254,211,44,255);
}

/* ComboBox style */
QComboBox {
	border-radius: 10%;
	background-color: rgba(32,36,42,255);
	color: white;
	padding-left: 10px;
	height: 40px;
}

QComboBox:on {
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 40px;
	background-color: rgba(254,211,44,255);
    border-top-right-radius: 10%;
    border-bottom-right-radius: 10%;
}

QComboBox::drop-down:on {
	border-bottom-left-radius: 0%;
	border-bottom-right-radius: 0%;
}

QComboBox::down-arrow {
    image: url(:/Icons/For_QT/chevron-down.svg);
}

QComboBox::down-arrow:on {
    image: url(:/Icons/For_QT/chevron-up.svg);
}

QComboBox QAbstractItemView {
	/* Style of the drop-down list */
	padding: 10px 5px;
	color: white;
    selection-color: black;
    selection-background-color: rgba(254,211,44,255);
	font-size: 10pt;
    box-shadow: transparent;
	background-color: rgba(32,36,42,255);
	border-bottom-left-radius: 10%;
	border-bottom-right-radius: 10%;
}

/* CheckBox style */
QCheckBox {
	color: white;
	border-radius: 10%;
}

QCheckBox::indicator:unchecked {
	image: url(:/Icons/For_QT/square.svg);
}

QCheckBox::indicator:checked {
	
	image: url(:/Icons/For_QT/check-square.svg);
}

/* Scroll Area style */
#scroll_widget {
	background-color: transparent;
	overflow: hidden;
}

QScrollArea, #scroll_area_container {
	background-color: transparent;
}

#scroll_area_content {
	background-color: rgb(76,79,86);
	border-radius: 10%;
}

/* TitleBar style */
#title_bar {
	background-color: rgb(76,79,86);
	border-top-left-radius: 10%;
	border-top-right-radius: 10%;
}

#window_title {
	font-size: 20px;
}

#title_bar QPushButton {
	height: 25px;
}

#icon {
	min-width: 25px;
	min-height: 25px;
	background: rgba(254,211,44,255);
	border-radius: 5%;
}

/* ScrollBar style */
/* VERTICAL SCROLLBAR */
 QScrollBar:vertical {
	border: 1px solid rgba(254,211,44,255);
	background-color: rgba(32,36,42,255);
    width: 20px;
	border-radius: 5%;
 }

/*  HANDLE BAR VERTICAL */
QScrollBar::handle:vertical {	
	background-color: rgb(76,79,86);
	min-height: 20px;
	margin: 20px 0 20px 0;
}
QScrollBar::handle:vertical:hover{	
	background: rgba(204,161, 44,255);
}

/* BTN TOP - SCROLLBAR */
QScrollBar::sub-line:vertical {
	border: none;
	background: rgba(254,211,44,255);
	height: 21px;
	border-top-left-radius: 3%;
	border-top-right-radius: 3%;
	subcontrol-position: top;
	subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical:hover {	
	background: rgba(204,161, 44,255);
}

/* BTN BOTTOM - SCROLLBAR */
QScrollBar::add-line:vertical {
	border: none;
	background: rgba(254,211,44,255);
	height: 21px;
	border-bottom-left-radius: 3%;
	border-bottom-right-radius: 3%;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
}
QScrollBar::add-line:vertical:hover {	
	background: rgba(204,161, 44,255);
}

/* RESET ARROW */
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
	background: none;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}

QTextBrowser {
	background-color: transparent;
	color: white;
	font-size: 16px;
	overflow: hidden;
}

#bbk_text, #udk_text, #isbn_text {
	max-height: 60px;
}

#annotation_text {
	font-size: 14px;
}
